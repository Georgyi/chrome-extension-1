{
  "manifest_version": 3,
  "name": "LinkedIn Greeting",
  "description": "Say hello someone",
  "version": "0.0.1",
  "action": {
    "default_popup": "index.html",
    "default_title": "Open"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "scripting",
    "storage",
    "tabs",
    "activeTab"
  ]
}